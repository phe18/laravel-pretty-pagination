{
    "name": "nid/laravel-pretty-pagination",
    "version": "1.0.0",
    "description": "A package for pretty pagination in Laravel",
    "type": "library",
    "require": {
        "php": "^8.2",
        "illuminate/container": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/pagination": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Nid\\LaravelPrettyPagination\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nid\\LaravelPrettyPagination\\Providers\\PrettyPaginationProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "license": "MIT"
}
